{
    "name": "serena-backend",
    "version": "0.0.0",
    "description": "Backend for Serena",
    "main": "dist/index.js",
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
        "ext": "ts, js"
    },
    "scripts": {
        "build": "tsc && tsc-alias",
        "start": "npm run build && node dist/index.js",
        "dev": "nodemon src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Serena-Smart-Home-Smart-Wellbeing/serena-backend.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Serena-Smart-Home-Smart-Wellbeing/serena-backend/issues"
    },
    "homepage": "https://github.com/Serena-Smart-Home-Smart-Wellbeing/serena-backend#readme",
    "dependencies": {
        "@google-cloud/secret-manager": "^5.0.1",
        "@prisma/client": "^5.6.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2"
    },
    "devDependencies": {
        "@mermaid-js/mermaid-cli": "^10.6.1",
        "@types/cors": "^2.8.16",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.9.2",
        "nodemon": "^3.0.1",
        "prisma": "^5.6.0",
        "prisma-erd-generator": "^1.11.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    }
}
